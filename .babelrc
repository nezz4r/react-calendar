{
  "presets": ["@babel/preset-react"],
  "plugins": [
    [
      "styled-components",
      {
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@hooks": "./src/hooks",
          "@components": "./src/components",
          "@helpers": "./src/helpers",
          "@layouts": "./src/layouts",
          "@styles": "./src/styles",
          "@helpers": "./src/helpers",
          "@icons": "./src/components/Icons/index.js"
        }
      }
    ]
  ]
}
